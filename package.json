{
  "name": "ReactWebpackSassStarter",
  "version": "1.0.0",
  "description": "Just a really simple starter to work off of for future react projects.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack-serve": "webpack-dev-server --open",
    "start": "concurrently --kill-others \"npm run watch-functions\" \"npm run watch\" \"firebase serve --only hosting,functions\"",
    "watch": "webpack --progress --watch",
    "watch-functions": "cd functions && npm run watch",
    "build": "set NODE_ENV=production&& webpack -p",
    "build-functions": "cd functions && npm run build && cd ..",
    "deploy": "npm run build && npm run build-functions && firebase deploy"
  },
  "author": "RaphaÃ«l Titsworth-Morin",
  "license": "ISC",
