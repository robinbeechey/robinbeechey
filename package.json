    "history": "^4.7.2",
    "moment": "^2.18.1",
    "prismic-dom": "^2.0.0",
    "prismic-javascript": "^1.2.0",
    "prismic-reactjs": "^0.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.3.5",
    "react-redux": "^5.0.6",
    "react-responsive-carousel": "^3.1.28",
    "react-router": "^4.2.0",
